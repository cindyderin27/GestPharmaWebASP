
@{
    ViewBag.Title = "Register";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>  Registration </title>
    <!-- for-mobile-apps -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="keywords" content="Auditions Registration Form Widget Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />
    <script type="application/x-javascript">
        addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
               function hideURLbar(){ window.scrollTo(0,1); } </script>
    <!-- //for-mobile-apps -->
    <!-- //custom-theme -->
    <link href="~/Content/Styles/styleRegister.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Content/Styles/jqueryRegsiter-ui.css" rel="stylesheet" />
    <!-- js -->
    <script src="~/Content/Scripts/jqueryRegister-2.1.4.min.js"></script>
    @*<script type="text/javascript" src="js/jquery-2.1.4.min.js"></script>*@
    <!-- //js -->
    <!-- font-awesome-icons -->
    <!-- //font-awesome-icons -->
    <link href="//fonts.googleapis.com/css?family=Anton&amp;subset=latin-ext,vietnamese" rel="stylesheet">
</head>
<body>
    <!-- banner -->
    <div class="center-container">
        <div class="banner-dott">
            <div class="main">
                <h1 class="w3layouts_head">Auditions Registration Form Cooproration  Kameni Leukoue </h1>
                <div class="w3layouts_main_grid">
                    <form action="#" method="post" class="w3_form_post">
                        <div class="w3_agileits_main_grid w3l_main_grid">
                            <span class="agileits_grid">
                                <label>First Name </label>
                                <input type="text" name="Task" placeholder="First Name" required="">
                            </span>
                        </div>
                        @*<div class="w3_agileits_main_grid w3l_main_grid">
                    <span class="agileits_grid">
                        <label>Last Name </label>
                        <input type="text" name="Task" placeholder="Last Name" required="">
                    </span>
                </div>*@
                        <div class="w3_agileits_main_grid w3l_main_grid">
                            <span class="agileits_grid">
                                <label>Your Email </label>
                                <input type="text" name="Task" placeholder="Your Email" required="">
                            </span>
                        </div>
                        <div class="w3_agileits_main_grid w3l_main_grid">
                            <span class="agileits_grid">
                                <label>Password </label>
                                <input type="password" name="Task" placeholder="Your password" required="">
                            </span>
                        </div>
                        <div class="w3_agileits_main_grid w3l_main_grid">
                            <span class="agileits_grid">
                                <label>Select your country </label>
                                <select name="country">
                                    <option value="none" selected="" disabled="">Select Category</option>
                                    <option value="Ph">Cameroun</option>
                                    <option value="Po">France</option>
                                    <option value="So">Espagne</option>
                                    <option value="Sp">Canada</option>
                                    <option value="Sp">Other's</option>

                                </select>
                            </span>
                        </div>
                        <div class="agileits_w3layouts_main_grid w3ls_main_grid">
                            <span class="agileinfo_grid">
                                <label>Date / Time </label>
                                <div class="agileits_w3layouts_main_gridl">
                                    <input class="date" id="datepicker" name="Text" type="text" value="mm/dd/yyyy" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '';}" required="">
                                </div>
                                <div class="agileits_w3layouts_main_gridr">
                                    <input type="time" name="Time" placeholder=" " required="">
                                </div>
                                <div class="clear"> </div>
                            </span>
                        </div>

                        <div class="w3_main_grid">
                            <div class="w3_main_grid_right">
                                <input type="submit" value="Submit">
                            </div>
                        </div>
                    </form>
                </div>
                <!-- Calendar -->
                @*<link rel="stylesheet" href="css/jquery-ui.css" />*@
                <link href="~/Content/Styles/jqueryRegsiter-ui.css" rel="stylesheet" />
                <script src="~/Content/Scripts/jquery-uiRegister.js"></script>
                @*<script src="js/jquery-ui.js"></script>*@
                <script src="~/Content/Scripts/jqueryRegister-2.1.4.min.js"></script>
                <script>

                    $(function () {
                        $("#datepicker").datepicker();
                    });
                </script>
                <!-- //Calendar -->
                <div class="w3layouts_copy_right">
                    <div class="container">
                        <p>© 2021 Auditions Registration Form. All rights reserved </a></p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- //footer -->
</body>
</html>
